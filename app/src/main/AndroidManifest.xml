<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.movie.rahulrv"
          xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name=".MyApplication"
        android:supportsRtl="true"
        android:theme="@style/MoviesNow">
        <activity android:name=".ui.movies.activities.HomeActivityJava"
        android:theme="@style/MoviesNow.Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ui.movies.activities.MovieDetailActivity"
                  android:parentActivityName=".ui.movies.activities.HomeActivityJava"/>

    </application>

</manifest>